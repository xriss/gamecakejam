#create sym links to dirs in the gateau
#removes all files first

cd `dirname $0`

rm -rf gateau
mkdir gateau

ids=( gthang dmazed batwsbat lonelybird hunted )

for id in "${ids[@]}" ; do

ln -s ../../../$id/art/icons/gateau gateau/$id

cp ../../$id/out/$id.zip ../test

done

ids=( bulb )

for id in "${ids[@]}" ; do

ln -s ../../../../apps/$id/art/icons/gateau gateau/$id

cp ../../../apps/$id/out/$id.zip ../test

done

./gateau_update
